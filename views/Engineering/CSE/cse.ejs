    <style>
        /* Reset */
        body,
        h1,
        h2,
        h3,
        p,
        ul {
            margin: 0;
            padding: 0;
        }

        body {
            font-family: 'Poppins', sans-serif;
            background: #0f172a;
            /* Dark blue background */
            color: #e2e8f0;
            /* Light text color */
        }

        /* Navbar */
        .navbar {
            background: linear-gradient(to right, #1e293b, #334155);
            /* Dark gradient */
            padding: 1rem 2rem;
            color: #fff;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
        }

        .nav-container {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .logo {
            font-size: 1.8rem;
            font-weight: bold;
            color: #38bdf8;
            /* Neon blue */
        }

        .back-button {
            text-decoration: none;
            color: #fff;
            font-size: 1rem;
            transition: color 0.3s ease;
        }

        .back-button:hover {
            color: #38bdf8;
        }

        @media (max-width: 768px) {
            .logo {
                font-size: 1.5rem;
            }
        }

        @media (max-width: 480px) {
            .navbar {
                flex-direction: column;
                text-align: center;
            }
        }

        /* Intro Section */
        .intro {
            padding: 50px 20px;
            text-align: center;
            background: linear-gradient(135deg, #1e293b, #0f172a);
            /* Dark gradient */
            color: #38bdf8;
            /* Neon blue */
        }

        .intro h1 {
            font-size: 3rem;
            margin-bottom: 15px;
        }

        .intro p {
            font-size: 1.2rem;
            color: #94a3b8;
            /* Muted text */
        }

        /* Real World Projects Section */
        .projects {
            padding: 50px 20px;
            text-align: center;
        }

        .projects h2 {
            color: #38bdf8;
        }

        .project-cards {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 20px;
            margin-top: 30px;
        }

        .project-card {
            background: #1e293b;
            /* Dark card background */
            padding: 20px;
            border-radius: 12px;
            box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2);
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }

        .project-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 12px 20px rgba(0, 0, 0, 0.3);
        }

        .project-card h3 {
            color: #38bdf8;
        }

        /* Quote Section */
        .quote {
            background: #0f172a;
            color: #38bdf8;
            padding: 40px 20px;
            text-align: center;
        }

        .quote p {
            font-size: 1.5rem;
            font-style: italic;
        }

            /* Study Paths Section */
    .study-paths {
        padding: 50px 20px;
        background: #1e293b;
        text-align: center;
    }

    .study-paths h2 {
        color: #38bdf8;
        font-size: 2.5rem;
        margin-bottom: 30px;
    }

    .paths-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
        gap: 25px;
        margin-top: 30px;
    }

    .path-card {
        background: #0f172a;
        padding: 20px;
        border-radius: 12px;
        transition: transform 0.3s ease, box-shadow 0.3s ease;
        box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2);
        text-align: center;
    }

    .path-card:hover {
        background: #1e293b;
        transform: translateY(-5px);
        box-shadow: 0 12px 20px rgba(0, 0, 0, 0.3);
    }

    .path-card img {
        width: 100%;
        height: 150px;
        object-fit: cover;
        border-radius: 8px;
        margin-bottom: 15px;
    }

    .path-card h3 {
        color: #38bdf8;
        font-size: 1.5rem;
        margin-bottom: 10px;
    }

    .path-card p {
        color: #94a3b8;
        font-size: 1rem;
        line-height: 1.5;
    }

    .path-card a {
        color: #38bdf8;
        text-decoration: none;
        font-weight: bold;
        transition: color 0.3s ease;
    }

    .path-card a:hover {
        color: #93c5fd;
    }

    /* Responsive Design */
    @media (max-width: 768px) {
        .paths-grid {
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
        }
    }

    @media (max-width: 480px) {
        .paths-grid {
            grid-template-columns: 1fr;
        }

        .path-card img {
            height: 120px;
        }
    }


          /* Top Colleges Section */
  .colleges {
    padding: 50px 20px;
    background: #1e293b; /* Dark background */
    text-align: center;
  }

  .colleges h2 {
    color: #38bdf8; /* Neon blue */
