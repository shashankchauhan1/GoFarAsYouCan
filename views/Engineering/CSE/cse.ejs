    margin-bottom: 20px;
  }

  .college-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 20px;
  }

  .college-card {
    background: #0f172a; /* Darker card background */
