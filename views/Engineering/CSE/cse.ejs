    margin-bottom: 10px;
  }

  .college-card p {
    color: #94a3b8;
    font-size: 1rem;
    margin-bottom: 15px;
  }

  .college-card a {
    color: #38bdf8;
    text-decoration: none;
    font-weight: bold;
    transition: color 0.3s ease;
  }

  .college-card a:hover {
